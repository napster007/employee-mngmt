abel=\"Contact No.\" name=\"cpnum\" onChange={this.handleChange}   value={item.cpnum } />\r\n              <Divider/>\r\n              <InputLabel htmlFor=\"component-simple\">Address</InputLabel>\r\n              <Input required id=\"standard-required\" label=\"Address\" name=\"address\" onChange={this.handleChange} value={item.address} />\r\n              <Divider/> \r\n              <InputLabel htmlFor=\"component-simple\">Email</InputLabel>\r\n               <Input required id=\"standard-required\" placeholder=\"Email\" name=\"email\" onChange={this.handleChange} value={item.email} />\r\n               <InputLabel htmlFor=\"component-simple\">Password</InputLabel>\r\n               <Input required id=\"standard-required\" placeholder=\"Password\" name=\"password\" autoComplete=\"off\" type=\"password\" onChange={this.handleChange} value={item.password} />\r\n               <Divider/>  \r\n                <button type=\"Submit\">Update Employee</button>\r\n               \r\n                <Button to=\"/employee-list\" id={item.key} onClick={this.handleCancel}>Cancel</Button>\r\n               \r\n\r\n               \r\n         \r\n             \r\n              </form>\r\n              </CardContent>\r\n         </Card>\r\n       \r\n      </div>\r\n    );\r\n  }\r\n}\r\nexport default withRouter(EmployeesData);\r\n\r\n",{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":1,"column":17,"nodeType":"80","messageId":"81","endLine":1,"endColumn":26},{"ruleId":"78","severity":1,"message":"82","line":20,"column":8,"nodeType":"80","messageId":"81","endLine":20,"endColumn":14},{"ruleId":"78","severity":1,"message":"83","line":72,"column":35,"nodeType":"80","messageId":"81","endLine":72,"endColumn":39},{"ruleId":"84","severity":1,"message":"85","line":156,"column":46,"nodeType":"86","endLine":156,"endColumn":51},{"ruleId":"78","severity":1,"message":"87","line":10,"column":10,"nodeType":"80","messageId":